[
{
    "key": "ctrl+shift+enter",
    "command": "r.sourceCurrentFileWithEcho",
    "when": "!isRPackage && editorLangId == 'r'"
},
{
    "key": "ctrl+shift+enter",
    "command": "-r.sourceCurrentFile",
    "when": "!isRPackage && editorLangId == 'r'"
},
{
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
        "text": "\\\r\n"
    },
    "when": "terminalFocus"
},
{
    "key": "Ctrl+. R",
    "command": "workbench.action.executeCode.console",
    "when": "editorTextFocus",
    "args": {
        "langId": "r",
        "code": "fnmate::rs_fnmate()",
        "focus": false
    }
},    
{
    "key": "Ctrl+B C",
    "command": "workbench.action.executeCode.console",
    "when": "editorTextFocus",
    "args": {
        "langId": "r",
        "code": "breakerofchains::break_chain()",
        "focus": false
    }
},
{
    "key": "Ctrl+. O",
    "command": "workbench.action.executeCode.console",
    "when": "editorTextFocus",
    "args": {
        "langId": "r",
        "code": "eval(parse(text = atcursor::get_word_or_selection()))",
        "focus": false
    }
},
// targets
{
    "key": "Ctrl+T M",
    "command": "workbench.action.executeCode.console",
    "args": {
        "langId": "r",
        "code": "targets::tar_make()",
        "focus": false
    }
},
{
    "key": "Ctrl+T T",
    "command": "workbench.action.executeCode.console",
    "args": {
        "langId": "r",
        "code": "bzipr::rstudio_addin_tar_make_target()",
        "focus": false
    }
},
{
    "key": "Ctrl+T G",
    "command": "workbench.action.executeCode.console",
    "args": {
        "langId": "r",
        "code": "targets::tar_load_globals()",
        "focus": false
    }
},
{
    "key": "Ctrl+T E",
    "command": "workbench.action.executeCode.console",
    "args": {
        "langId": "r",
        "code": "targets::tar_load_everything()",
        "focus": false
    }
},
{
    "key": "Ctrl+T L",
    "command": "workbench.action.executeCode.console",
    "args": {
        "langId": "r",
        "code": "targets::rstudio_addin_tar_load()",
        "focus": false
    }
},
{
    "key": "Ctrl+T R",
    "command": "workbench.action.executeCode.console",
    "args": {
        "langId": "r",
        "code": "targets::rstudio_addin_tar_read()",
        "focus": false
    }
}
]