[
{
    "key": "ctrl+shift+enter",
    "command": "r.sourceCurrentFileWithEcho",
    "when": "!isRPackage && editorLangId == 'r'"
},
{
    "key": "ctrl+shift+enter",
    "command": "-r.sourceCurrentFile",
    "when": "!isRPackage && editorLangId == 'r'"
},
{
    "key": "Ctrl+. Ctrl+R",
    "command": "workbench.action.executeCode.console",
    "when": "editorTextFocus",
    "args": {
        "langId": "r",
        "code": "fnmate::rs_fnmate()",
        "focus": false
    }
},    
{
    "key": "Ctrl+B Ctrl+C",
    "command": "workbench.action.executeCode.console",
    "when": "editorTextFocus",
    "args": {
        "langId": "r",
        "code": "breakerofchains::break_chain()",
        "focus": false
    }
},
{
    "key": "Ctrl+. Ctrl+O",
    "command": "workbench.action.executeCode.console",
    "when": "editorTextFocus",
    "args": {
        "langId": "r",
        "code": "eval(parse(text = atcursor::get_word_or_selection()))",
        "focus": false
    }
},
// targets
